<div id="Happy" class="tabcontentB">

<h3>Your Happy Posts</h3>
 <p>{{#each posts}}
	<div class="post" id="{{id}}">
		
		<!-- If positive post -->
		{{#if happy}}

		<img src="{{emote}}" class="postEmote"></img>
		<h5>You were feeling {{mood}} at {{date}}.</h5>


		<!-- If media is attached -->
		<!-- If photo is attached -->
		{{# if photoOn}}
			<img src ="{{photo}}" class="postPhoto">
		{{/if}}

		<!-- If video is attached -->
		{{# if videoOn}}
		<video class="postVideo" controls>
			<source src="{{video}}" type="video/mp4">
		</video>
		{{/if}}

		<!-- If audio is attached -->
		{{# if audioOn}}
		<audio class="postAudio" controls>
			<source src="{{audio}}" type="audio/mpeg">			
		</audio>
		{{/if}}

		<!-- If text is attached -->
		{{# if textOn}}
			<object class="postText" 
			data={{text}}
			></object>

		{{/if}}

		<!-- End if media -->

		<hr>
		{{/if}}
		
		<!-- End if positive -->

	</div>
	{{/each}}</p>
</div>



