<!-- Top fixed nav bar -->
<div id="navbartop">
<button class="tablinkB" onclick="openCity('Happy', this, '#FFA8A7')"
			 id="defaultOpen"><img class="iconimgtop" 
			 src="/emotes/happy4.png"></button>
<button class="tablinkB" onclick="openCity('Loving', this, '#FFA8A7')"><img class="iconimgtop" 
			 src="/emotes/loving1.png"></button>
