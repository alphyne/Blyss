<!-- Top fixed nav bar -->
<div id="navbartop">
<button class="tablinkA" onclick="openCity('Happy', this, 'green')"
			 id="defaultOpen"><img class="iconimgtop" 
			 src="/emotes/happy4.png"></button>
<button class="tablinkA" onclick="openCity('Loving', this, 'green')"><img class="iconimgtop" 
			 src="/emotes/loving1.png"></button>
<button class="tablinkA" onclick="openCity('Relaxed', this, 'green')"><img class="iconimgtop" 
			 src="/emotes/relaxed4.png"></button>
<button class="tablinkA" onclick="openCity('Sad', this, 'green')"><img class="iconimgtop" 
			 src="/emotes/sad1.png"></button>
<button class="tablinkA" onclick="openCity('Upset', this, 'green')"><img class="iconimgtop" 
